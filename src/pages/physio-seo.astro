---
import BaseLayout from '../layouts/BaseLayout.astro';
import Header from '../components/layout/Header.astro';
import Footer from '../components/layout/Footer.astro';
import HeroHome from '../components/sections/HeroHome.astro';
import ClientLogoCarousel from '../components/sections/ClientLogoCarousel.astro';
import CaseStudiesSection from '../components/sections/CaseStudiesSection.astro';
import CTASection from '../components/sections/CTASection.astro';
import SchemaOrg from '../components/seo/SchemaOrg.astro';
import AccordionItem from '../components/ui/AccordionItem.astro';

// ===================================
// PAGE FRONTMATTER CONFIGURATION
// Edit these values to customize the page content
// ===================================

// SEO Meta Data
const seo = {
  title: 'Physio SEO Services | Attract More Patients in 2025',
  description: 'Expert SEO for physiotherapists and physio clinics. Dominate local search, attract new patients, and grow your physiotherapy practice with our proven SEO strategies.',
  keywords: [
    'physio seo',
    'seo for physio',
    'physio website seo',
    'seo for physiotherapist',
    'physiotherapy seo',
    'seo for physiotherapists',
    'physio clinic seo',
    'physiotherapy practice seo',
    'seo for physios'
  ],
  ogImage: '/images/team-hero.png',
  ogType: 'website',
};

// Hero Section
const hero = {
  title: 'Physio SEO That Fills Your Appointment Book',
  subtitle: 'Attract more patients searching for physiotherapy services in your area. Our proven SEO strategies help Australian physio clinics dominate local search and grow their patient base.',
  primaryCTA: {
    text: 'Free Strategy Session',
    href: '/contact/'
  },
};

// Breadcrumbs
const breadcrumbs = [
  { label: 'Home', href: '/' },
  { label: 'Services', href: '/services/' },
  { label: 'Physio SEO', href: '/physio-seo/' },
];

// Section Visibility (show/hide sections)
const sections = {
  showClientLogos: true,
  showCaseStudies: true,
  showWhatsIncluded: true,
  showProcess: true,
  showFinalCTA: true,
};

// Final CTA Section
const finalCTA = {
  title: 'Ready to Attract More Patients and Grow Your Physiotherapy Practice?',
  subtitle: 'Get your free physio SEO audit and discover exactly how we\'ll help you dominate local search, attract new patients, and fill your appointment book. No obligations, just actionable insights.',
  primaryCTA: {
    text: 'Free Strategy Session',
    href: '/contact/'
  },
};
---

<BaseLayout
  title={seo.title}
  description={seo.description}
  keywords={seo.keywords}
  ogImage={seo.ogImage}
  ogType={seo.ogType}
  schema={{
    type: 'Service',
    data: {
      '@type': 'Service',
      name: 'Physio SEO Services',
      description: seo.description,
      provider: {
        '@type': 'Organization',
        name: 'Digital Growth Experts',
      },
      areaServed: {
        '@type': 'Country',
        name: 'Australia',
      },
      hasOfferCatalog: {
        '@type': 'OfferCatalog',
        name: 'Physio SEO Services',
        itemListElement: [
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Physiotherapy Website Optimization',
            },
          },
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Local SEO for Physio Clinics',
            },
          },
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Physio Content Marketing',
            },
          },
        ],
      },
    },
  }}
>
  <!-- Add LocalBusiness schema for better local SEO -->
  <SchemaOrg
    slot="head"
    type="LocalBusiness"
    data={{
      name: 'Digital Growth Experts',
      description: 'Expert SEO services for physiotherapists and physio clinics across Australia',
      image: `${Astro.site}images/logo.png`,
    }}
  />

  <Header />

  <main id="main-content">
    <!-- Hero Section - Conversion-focused with clear value proposition -->
    <HeroHome
      title={hero.title}
      subtitle={hero.subtitle}
      primaryCTA={hero.primaryCTA}
      secondaryCTA={hero.secondaryCTA}
    />

    <!-- Social Proof - Client Logos -->
    {sections.showClientLogos && <ClientLogoCarousel />}

    <!-- Proven Results Section - Case Studies with Real Data -->
    {sections.showCaseStudies && <CaseStudiesSection />}

    <!-- What's Included Section - Service Details -->
    {sections.showWhatsIncluded && (
    <section class="section-padding bg-gray-50">
      <div class="container-custom">
        <div class="max-w-3xl mx-auto text-center mb-16">
          <h2 class="text-3xl md:text-4xl lg:text-5xl font-bold font-display text-gray-900 mb-4">
            What's Included in Our Physio SEO Services
          </h2>
          <p class="text-lg md:text-xl text-gray-600">
            Everything you need to dominate local search and attract more patients to your physiotherapy clinic.
          </p>
        </div>

        <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8 max-w-6xl mx-auto">
          <!-- Service 1 -->
          <div class="bg-white p-8 rounded-xl shadow-md hover:shadow-lg transition-shadow">
            <div class="w-14 h-14 bg-primary-100 rounded-lg flex items-center justify-center mb-4">
              <svg
                class="w-7 h-7 text-primary-600"
                fill="none"
                stroke="currentColor"
                viewBox="0 0 24 24"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"
                />
              </svg>
            </div>
            <h3 class="text-xl font-bold font-display text-gray-900 mb-3">Comprehensive SEO Audit</h3>
            <p class="text-gray-600 mb-4">
              In-depth analysis of your physiotherapy website's current SEO performance, technical issues, and opportunities to improve your search rankings and attract more patients. <a href="/"><strong>Digital Growth Experts</strong></a> specializes in healthcare practice SEO.
            </p>
          </div>

          <!-- Service 2 -->
          <div class="bg-white p-8 rounded-xl shadow-md hover:shadow-lg transition-shadow">
            <div class="w-14 h-14 bg-secondary-100 rounded-lg flex items-center justify-center mb-4">
