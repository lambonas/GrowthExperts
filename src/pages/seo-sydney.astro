---
import BaseLayout from '../layouts/BaseLayout.astro';
import Header from '../components/layout/Header.astro';
import Footer from '../components/layout/Footer.astro';
import HeroHome from '../components/sections/HeroHome.astro';
import ClientLogoCarousel from '../components/sections/ClientLogoCarousel.astro';
import CaseStudiesSection from '../components/sections/CaseStudiesSection.astro';
import CTASection from '../components/sections/CTASection.astro';
import SchemaOrg from '../components/seo/SchemaOrg.astro';

// ===================================
// PAGE FRONTMATTER CONFIGURATION
// Edit these values to customize the page content
// ===================================

// SEO Meta Data
const seo = {
  title: 'SEO Sydney | Expert SEO Company & Agency Services',
  description: 'Leading SEO Sydney agency delivering proven results. Expert SEO company Sydney specialising in technical SEO, local search & enterprise strategies.',
  keywords: [
    'Seo Sydney',
    'SEO company Sydney',
    'SEO Agency Sydney',
    'Sydney SEO',
    'Best Enterprise SEO Agency Sydney',
    'SEO Marketing Sydney',
    'SEO Experts Sydney',
    'Sydney SEO Agency'
  ],
  ogImage: '/images/team-hero.png',
  ogType: 'website',
};

// Hero Section
const hero = {
  title: 'SEO Sydney',
  subtitle: 'We\'ve helped businesses across Sydney dominate page one for their most valuable keywords. From local trades to enterprise brands, our technical SEO and no-BS strategy are built to drive leads, not vanity traffic.',
  primaryCTA: {
    text: 'Free Strategy Session',
    href: '/contact/'
  },
};

// Breadcrumbs
const breadcrumbs = [
  { label: 'Home', href: '/' },
  { label: 'Services', href: '/services/' },
  { label: 'SEO Sydney', href: '/seo-sydney/' },
];

// Section Visibility (show/hide sections)
const sections = {
  showClientLogos: true,
  showCaseStudies: true,
  showWhatsIncluded: true,
  showProcess: true,
  showFinalCTA: true,
};

// Final CTA Section
const finalCTA = {
  title: 'Ready to Dominate Sydney Search Results?',
  subtitle: 'Join the Sydney businesses achieving first page rankings and driving growth through expert SEO. Get your free audit and custom strategy today.',
  primaryCTA: {
    text: 'Free Strategy Session',
    href: '/contact/'
  },
};
---

<BaseLayout
  title={seo.title}
  description={seo.description}
  keywords={seo.keywords}
  ogImage={seo.ogImage}
  ogType={seo.ogType}
  schema={{
    type: 'Service',
    data: {
      '@type': 'Service',
      name: 'SEO Services Sydney',
      description: seo.description,
      provider: {
        '@type': 'Organization',
        name: 'Digital Growth Experts',
      },
      areaServed: {
        '@type': 'City',
        name: 'Sydney',
        containedIn: {
          '@type': 'State',
          name: 'New South Wales',
          containedIn: {
            '@type': 'Country',
            name: 'Australia'
          }
        }
      },
      hasOfferCatalog: {
        '@type': 'OfferCatalog',
        name: 'Sydney SEO Services',
        itemListElement: [
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Technical SEO Sydney',
            },
          },
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Local SEO Sydney',
            },
          },
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Enterprise SEO Sydney',
            },
          },
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Ecommerce SEO Sydney',
            },
          },
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'SEO Content Marketing Sydney',
            },
          },
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Link Building Sydney',
            },
          },
        ],
      },
    },
  }}
>
  <!-- Add LocalBusiness schema for better local SEO -->
  <SchemaOrg
    slot="head"
    type="LocalBusiness"
    data={{
      name: 'Digital Growth Experts',
      description: 'Premier SEO agency Sydney delivering proven search engine optimisation results for businesses across Australia',
      image: `${Astro.site}images/logo.png`,
      address: {
        '@type': 'PostalAddress',
        addressLocality: 'Sydney',
        addressRegion: 'NSW',
        addressCountry: 'AU'
      }
    }}
  />

  <Header />

  <main id="main-content">
    <!-- Hero Section - Conversion-focused with clear value proposition -->
    <HeroHome
      title={hero.title}
      subtitle={hero.subtitle}
      primaryCTA={hero.primaryCTA}
      secondaryCTA={hero.secondaryCTA}
    />

    <!-- Social Proof - Client Logos -->
    {sections.showClientLogos && <ClientLogoCarousel />}

    <!-- Initial Content Section - Introduction to SEO Sydney -->
    <section class="section-padding bg-white">
      <div class="container-custom">
        <div class="max-w-4xl mx-auto">
          <!-- Section Header -->
          <div class="text-center mb-12">
            <h2 class="text-3xl md:text-4xl lg:text-5xl font-bold font-display text-gray-900 mb-4">
              Why Sydney Businesses Choose Our SEO Agency
            </h2>
            <p class="text-lg md:text-xl text-gray-600 leading-relaxed">
              Proven SEO strategies that deliver measurable results for Sydney businesses.
            </p>
          </div>

          <!-- Key Benefits Grid -->
          <div class="grid md:grid-cols-2 gap-8 mb-12">
            <div class="flex gap-4">
              <div class="flex-shrink-0">
                <div class="w-12 h-12 bg-primary-100 rounded-lg flex items-center justify-center">
                  <svg
                    class="w-6 h-6 text-primary-600"
                    fill="none"
                    stroke="currentColor"
                    viewBox="0 0 24 24"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      stroke-width="2"
                      d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"
                    />
                  </svg>
                </div>
              </div>
              <div>
                <h3 class="text-xl font-bold font-display text-gray-900 mb-2">Proven SEO Results</h3>
                <p class="text-gray-600">
