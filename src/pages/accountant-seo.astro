---
import BaseLayout from '../layouts/BaseLayout.astro';
import Header from '../components/layout/Header.astro';
import Footer from '../components/layout/Footer.astro';
import HeroHome from '../components/sections/HeroHome.astro';
import ClientLogoCarousel from '../components/sections/ClientLogoCarousel.astro';
import CaseStudiesSection from '../components/sections/CaseStudiesSection.astro';
import CTASection from '../components/sections/CTASection.astro';
import AccordionItem from '../components/ui/AccordionItem.astro';
import { teamHeroImage } from '../assets/images/index';

const seo = {
  title: 'Accountant SEO Services | Get More High-Value Clients',
  description: 'Specialized SEO services for accountants, CPAs, and accounting firms. Attract high-value clients searching for tax services, bookkeeping, and financial advice. Proven strategies for accounting practices.',
  keywords: [
    'accountant seo',
    'accounting firm seo',
    'cpa seo services',
    'tax accountant seo',
    'bookkeeper seo',
    'accounting practice marketing',
    'seo for accountants',
    'accounting firm marketing',
    'cpa marketing services',
  ],
  ogimage: teamHeroImage,
  ogType: 'website',
};

const hero = {
  title: 'Accountant SEO Services That Attract High-Value Clients',
  subtitle: 'Stop competing on price. Dominate search results and attract business owners, high-net-worth individuals, and corporate clients actively searching for accounting expertise.',
  primaryCTA: {
    text: 'Free Strategy Session',
    href: '/contact/',
  },
  image: teamHeroImage,
  imageAlt: 'SEO services for accountants and accounting firms',
};

const schema = {
  type: 'Service' as const,
  data: {
    '@type': 'Service',
    name: 'Accountant SEO Services',
    description: 'Specialized SEO and digital marketing services for accountants, CPAs, and accounting firms to attract high-value clients through organic search.',
    provider: {
      '@type': 'Organization',
      name: 'Digital Growth Experts',
    },
    areaServed: {
      '@type': 'Country',
      name: 'Australia',
    },
    hasOfferCatalog: {
      '@type': 'OfferCatalog',
      name: 'Accountant SEO Services',
      itemListElement: [
        {
          '@type': 'Offer',
          itemOffered: {
            '@type': 'Service',
            name: 'Local SEO for Accountants',
            description: 'Optimize your Google Business Profile and local listings to dominate "accountant near me" searches.',
          },
        },
        {
          '@type': 'Offer',
          itemOffered: {
            '@type': 'Service',
            name: 'Content Marketing for CPAs',
            description: 'Authority-building content that positions you as the go-to expert for tax planning, business advisory, and financial services.',
          },
        },
        {
          '@type': 'Offer',
          itemOffered: {
            '@type': 'Service',
            name: 'Technical SEO Optimization',
            description: 'Ensure your accounting website is fast, secure, and optimized for conversions and search engine visibility.',
          },
        },
      ],
    },
  },
};
---

<BaseLayout
  title={seo.title}
  description={seo.description}
  ogImage={seo.ogImage}
  ogType={seo.ogType}
  keywords={seo.keywords}
  schema={schema}
>
  <Header />

  <main id="main-content">
    <!-- Hero Section -->
    <HeroHome
      title={hero.title}
      subtitle={hero.subtitle}
      primaryCTA={hero.primaryCTA}
      secondaryCTA={hero.secondaryCTA}
    />

    <!-- Social Proof - Client Logos -->
    <ClientLogoCarousel />

    <!-- Why Accountant SEO Matters -->
    <section class="py-16 bg-white">
      <div class="container mx-auto px-4 max-w-7xl">
        <div class="max-w-3xl mx-auto text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold font-display mb-6 text-gray-900">
            Why SEO Is Critical for Accounting Firms
          </h2>
          <p class="text-xl text-gray-700 leading-relaxed">
            Your ideal clients are searching for accounting services online right now. If you're not ranking at the top, they're choosing your competitors.
          </p>
        </div>

        <!-- Benefits Grid -->
        <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-8">
          <div class="bg-gradient-to-br from-blue-50 to-indigo-50 p-6 rounded-xl">
            <div class="w-12 h-12 bg-blue-600 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z" />
              </svg>
            </div>
            <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Attract Ready-to-Buy Clients</h3>
            <p class="text-gray-700">
              73% of people searching for "accountant near me" or "CPA services" contact a firm within 24 hours. Capture high-intent prospects.
            </p>
          </div>

          <div class="bg-gradient-to-br from-green-50 to-emerald-50 p-6 rounded-xl">
            <div class="w-12 h-12 bg-green-600 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
              </svg>
            </div>
            <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Higher Client Lifetime Value</h3>
            <p class="text-gray-700">
              SEO attracts clients with ongoing needs—bookkeeping, tax planning, advisory services—not just one-time tax returns.
            </p>
          </div>

          <div class="bg-gradient-to-br from-purple-50 to-pink-50 p-6 rounded-xl">
            <div class="w-12 h-12 bg-purple-600 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z" />
              </svg>
            </div>
            <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Build Trust & Authority</h3>
            <p class="text-gray-700">
              Ranking at the top signals credibility. Prospects trust Google's judgment—first-page firms are seen as industry leaders.
            </p>
          </div>

          <div class="bg-gradient-to-br from-orange-50 to-red-50 p-6 rounded-xl">
            <div class="w-12 h-12 bg-orange-600 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7h8m0 0v8m0-8l-8 8-4-4-6 6" />
              </svg>
            </div>
            <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Outperform Paid Ads</h3>
            <p class="text-gray-700">
              SEO delivers 5.3x higher ROI than Google Ads for accounting services. Build sustainable visibility without ongoing ad spend.
            </p>
          </div>
        </div>
      </div>
    </section>

    <!-- Proven Results - Case Studies -->
    <CaseStudiesSection />

    <!-- What's Included Section -->
    <section class="py-16 bg-gray-50">
      <div class="container mx-auto px-4 max-w-7xl">
        <div class="max-w-3xl mx-auto text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold font-display mb-6 text-gray-900">
            What's Included in Our Accountant SEO Services
          </h2>
          <p class="text-xl text-gray-700">
            Comprehensive SEO strategies tailored specifically for accounting practices and CPA firms.
          </p>
        </div>

        <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8">
          <!-- Service Card 1 -->
          <div class="bg-white p-8 rounded-xl shadow-sm hover:shadow-md transition-shadow">
            <div class="flex items-start mb-4">
              <div class="flex-shrink-0">
                <div class="w-12 h-12 bg-blue-100 rounded-lg flex items-center justify-center">
                  <svg class="w-6 h-6 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
                  </svg>
                </div>
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Local SEO Domination</h3>
                <p class="text-gray-700">
                  Optimize your Google Business Profile, build local citations, and dominate "accountant near me" searches. We'll get you in the local 3-pack where 44% of clicks go.
                </p>
              </div>
            </div>
          </div>

          <!-- Service Card 2 -->
          <div class="bg-white p-8 rounded-xl shadow-sm hover:shadow-md transition-shadow">
            <div class="flex items-start mb-4">
              <div class="flex-shrink-0">
                <div class="w-12 h-12 bg-green-100 rounded-lg flex items-center justify-center">
                  <svg class="w-6 h-6 text-green-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z" />
                  </svg>
                </div>
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Authority <a href="/content-marketing/"><strong>Content Strategy</strong></a></h3>
                <p class="text-gray-700">
                  Create expert content targeting high-value keywords like "tax planning strategies," "business tax deductions," and "CFO services." Build topical authority in your niche.
                </p>
              </div>
            </div>
          </div>

          <!-- Service Card 3 -->
          <div class="bg-white p-8 rounded-xl shadow-sm hover:shadow-md transition-shadow">
            <div class="flex items-start mb-4">
              <div class="flex-shrink-0">
                <div class="w-12 h-12 bg-purple-100 rounded-lg flex items-center justify-center">
                  <svg class="w-6 h-6 text-purple-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z" />
                  </svg>
                </div>
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Service Page Optimization</h3>
                <p class="text-gray-700">
                  Optimize dedicated pages for each service—tax preparation, bookkeeping, payroll, business advisory, audit support—to capture specific search intent.
                </p>
              </div>
            </div>
          </div>

          <!-- Service Card 4 -->
          <div class="bg-white p-8 rounded-xl shadow-sm hover:shadow-md transition-shadow">
            <div class="flex items-start mb-4">
              <div class="flex-shrink-0">
                <div class="w-12 h-12 bg-indigo-100 rounded-lg flex items-center justify-center">
                  <svg class="w-6 h-6 text-indigo-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5.882V19.24a1.76 1.76 0 01-3.417.592l-2.147-6.15M18 13a3 3 0 100-6M5.436 13.683A4.001 4.001 0 017 6h1.832c4.1 0 7.625-1.234 9.168-3v14c-1.543-1.766-5.067-3-9.168-3H7a3.988 3.988 0 01-1.564-.317z" />
                  </svg>
                </div>
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Review & Reputation Management</h3>
                <p class="text-gray-700">
                  Implement systems to generate 5-star Google reviews. 88% of prospects check reviews before choosing an accountant—we'll help you build social proof.
                </p>
              </div>
            </div>
          </div>

          <!-- Service Card 5 -->
          <div class="bg-white p-8 rounded-xl shadow-sm hover:shadow-md transition-shadow">
            <div class="flex items-start mb-4">
              <div class="flex-shrink-0">
                <div class="w-12 h-12 bg-yellow-100 rounded-lg flex items-center justify-center">
                  <svg class="w-6 h-6 text-yellow-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 20l4-16m4 4l4 4-4 4M6 16l-4-4 4-4" />
                  </svg>
                </div>
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Technical SEO & Site Speed</h3>
                <p class="text-gray-700">
                  Ensure your website is fast, secure (HTTPS), mobile-friendly, and technically perfect. Fix crawl errors, optimize schema markup, and improve Core Web Vitals.
                </p>
              </div>
            </div>
          </div>

          <!-- Service Card 6 -->
          <div class="bg-white p-8 rounded-xl shadow-sm hover:shadow-md transition-shadow">
            <div class="flex items-start mb-4">
              <div class="flex-shrink-0">
                <div class="w-12 h-12 bg-red-100 rounded-lg flex items-center justify-center">
                  <svg class="w-6 h-6 text-red-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1" />
                  </svg>
                </div>
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Strategic Link Building</h3>
                <p class="text-gray-700">
                  Earn high-authority backlinks from finance publications, business directories, and industry associations to boost domain authority and rankings.
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Process Section -->
    <section class="py-16 bg-white">
      <div class="container mx-auto px-4 max-w-7xl">
        <div class="max-w-3xl mx-auto text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold font-display mb-6 text-gray-900">
            Our Proven SEO Process for Accountants
          </h2>
          <p class="text-xl text-gray-700">
            A systematic approach designed specifically for accounting firms to attract high-value clients.
          </p>
        </div>

        <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8 mt-12">
          <!-- Step 1 -->
          <div class="relative">
            <div class="flex items-start">
              <div class="flex-shrink-0 w-12 h-12 bg-blue-600 text-white rounded-full flex items-center justify-center font-bold text-lg">
                1
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Competitive Analysis & Audit</h3>
                <p class="text-gray-700">
                  We analyze your top-ranking competitors, audit your current SEO performance, and identify gaps and opportunities in your market. You'll see exactly where you stand and what it takes to dominate.
                </p>
              </div>
            </div>
          </div>

          <!-- Step 2 -->
          <div class="relative">
            <div class="flex items-start">
              <div class="flex-shrink-0 w-12 h-12 bg-blue-600 text-white rounded-full flex items-center justify-center font-bold text-lg">
                2
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Keyword Research & Strategy</h3>
                <p class="text-gray-700">
                  Identify high-value keywords your ideal clients are searching for—from "small business accountant" to "self-managed super fund advice." We target both local and service-specific terms.
                </p>
              </div>
            </div>
          </div>

          <!-- Step 3 -->
          <div class="relative">
            <div class="flex items-start">
              <div class="flex-shrink-0 w-12 h-12 bg-blue-600 text-white rounded-full flex items-center justify-center font-bold text-lg">
                3
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">On-Page SEO Optimization</h3>
                <p class="text-gray-700">
                  Optimize title tags, meta descriptions, headers, and content across your website. Implement schema markup for services, reviews, and local business data to enhance search visibility.
                </p>
              </div>
            </div>
          </div>

          <!-- Step 4 -->
          <div class="relative">
            <div class="flex items-start">
              <div class="flex-shrink-0 w-12 h-12 bg-blue-600 text-white rounded-full flex items-center justify-center font-bold text-lg">
                4
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Content Creation & Authority Building</h3>
                <p class="text-gray-700">
                  Create expert content that answers your prospects' questions—tax guides, financial planning articles, and industry insights. Establish your firm as the trusted authority.
                </p>
              </div>
            </div>
          </div>

          <!-- Step 5 -->
          <div class="relative">
            <div class="flex items-start">
              <div class="flex-shrink-0 w-12 h-12 bg-blue-600 text-white rounded-full flex items-center justify-center font-bold text-lg">
                5
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Local SEO & Citation Building</h3>
                <p class="text-gray-700">
                  Optimize your Google Business Profile, build citations on key directories (CPA Australia, local chambers), and ensure NAP consistency to dominate local search results.
                </p>
              </div>
            </div>
          </div>

          <!-- Step 6 -->
          <div class="relative">
            <div class="flex items-start">
              <div class="flex-shrink-0 w-12 h-12 bg-blue-600 text-white rounded-full flex items-center justify-center font-bold text-lg">
                6
              </div>
              <div class="ml-4">
                <h3 class="text-xl font-semibold font-display mb-2 text-gray-900">Monthly Reporting & Optimization</h3>
                <p class="text-gray-700">
                  Track rankings, organic traffic, leads, and conversions. Receive detailed monthly reports and ongoing optimizations to ensure continuous growth and ROI.
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Challenges Section -->
    <section class="py-16 bg-gradient-to-br from-blue-50 to-indigo-50">
      <div class="container mx-auto px-4 max-w-7xl">
        <div class="max-w-3xl mx-auto text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold font-display mb-6 text-gray-900">
            Common SEO Challenges for Accountants (And How We Solve Them)
          </h2>
        </div>

        <div class="grid md:grid-cols-2 gap-8 max-w-5xl mx-auto">
          <div class="bg-white p-8 rounded-xl shadow-sm">
            <h3 class="text-xl font-semibold font-display mb-3 text-gray-900">Challenge: High Competition in Major Cities</h3>
            <p class="text-gray-700 mb-4">
              Accounting SEO is competitive, especially in Sydney, Melbourne, and Brisbane where hundreds of firms compete for the same keywords.
            </p>
            <p class="text-gray-900 font-semibold">Our Solution:</p>
            <p class="text-gray-700">
              We target long-tail, service-specific keywords and hyper-local terms that your competitors overlook. Focus on specialty niches (construction accounting, medical practice bookkeeping) for faster wins.
            </p>
          </div>

          <div class="bg-white p-8 rounded-xl shadow-sm">
            <h3 class="text-xl font-semibold font-display mb-3 text-gray-900">Challenge: Seasonal Search Volume</h3>
            <p class="text-gray-700 mb-4">
              Tax-related searches spike before EOFY (June 30) but drop significantly afterward, creating inconsistent traffic.
            </p>
            <p class="text-gray-900 font-semibold">Our Solution:</p>
            <p class="text-gray-700">
              Build year-round visibility with content targeting ongoing services—bookkeeping, payroll, business advisory, BAS lodgement—not just tax returns.
            </p>
          </div>

          <div class="bg-white p-8 rounded-xl shadow-sm">
            <h3 class="text-xl font-semibold font-display mb-3 text-gray-900">Challenge: Low Organic CTR Due to Ads</h3>
            <p class="text-gray-700 mb-4">
              Google Ads often dominate accounting-related searches, pushing organic results below the fold and reducing click-through rates.
            </p>
            <p class="text-gray-900 font-semibold">Our Solution:</p>
            <p class="text-gray-700">
              Optimize for rich snippets, FAQ schema, and local pack visibility. Target informational queries where ads don't appear. Build brand recognition so users scroll past ads to find you.
            </p>
          </div>

          <div class="bg-white p-8 rounded-xl shadow-sm">
            <h3 class="text-xl font-semibold font-display mb-3 text-gray-900">Challenge: Demonstrating ROI to Partners</h3>
            <p class="text-gray-700 mb-4">
              Accounting firm partners need to see clear ROI metrics before investing in SEO, but results take 3-6 months to materialize.
            </p>
            <p class="text-gray-900 font-semibold">Our Solution:</p>
            <p class="text-gray-700">
              Transparent monthly reporting with tracked phone calls, form submissions, and client acquisition costs. We show early wins (quick ranking improvements, traffic growth) while building long-term authority.
            </p>
          </div>
        </div>
      </div>
    </section>

    <!-- Why Choose Us Section -->
    <section class="py-16 bg-white">
      <div class="container mx-auto px-4 max-w-7xl">
        <div class="max-w-3xl mx-auto text-center mb-12">
          <h2 class="text-3xl md:text-4xl font-bold font-display mb-6 text-gray-900">
            Why Accounting Firms Choose Digital Growth Experts
          </h2>
        </div>

        <div class="grid md:grid-cols-3 gap-8 max-w-6xl mx-auto">
          <div class="bg-gray-50 p-8 rounded-xl">
            <div class="w-14 h-14 bg-blue-600 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-7 h-7 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z" />
              </svg>
            </div>
            <h3 class="text-xl font-semibold font-display mb-3 text-gray-900">Industry Specialization</h3>
            <p class="text-gray-700">
              We understand the unique challenges of marketing professional services in compliance-heavy industries. Our strategies align with CPA Australia and regulatory requirements.
            </p>
          </div>

          <div class="bg-gray-50 p-8 rounded-xl">
            <div class="w-14 h-14 bg-green-600 rounded-lg flex items-center justify-center mb-4">
              <svg class="w-7 h-7 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7h8m0 0v8m0-8l-8 8-4-4-6 6" />
              </svg>
            </div>
            <h3 class="text-xl font-semibold font-display mb-3 text-gray-900">Proven Track Record</h3>
            <p class="text-gray-700">
              We've helped accounting firms across Australia increase organic traffic by 300%+ and generate a consistent pipeline of high-value leads every month.
            </p>
          </div>

          <div class="bg-gray-50 p-8 rounded-xl">
