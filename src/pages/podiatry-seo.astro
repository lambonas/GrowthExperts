  secondaryCTA: {
    text: 'View Results',
    href: '#results',
  },
  image: '/images/team-hero.png',
  imageAlt: 'SEO services for podiatrists and foot clinics',
};

const schema = {
  type: 'Service' as const,
  data: {
    '@type': 'Service',
    name: 'Podiatry SEO Services',
    description: 'Specialized SEO services for podiatrists and foot clinics to attract more patients and fill appointment schedules.',
    provider: {
      '@type': 'Organization',
      name: 'Digital Growth Experts',
    },
    areaServed: {
      '@type': 'Country',
      name: 'Australia',
    },
  },
