    'podiatry seo',
    'podiatry seo services',
    'podiatry seo experts',
    'seo for podiatrist',
    'seo for podiatrists',
    'podiatry practice',
    'search engine optimization',
    'local seo',
    'podiatry clinic',
    'seo for podiatry',
  ],
  canonical: 'https://growthexperts.com.au/podiatry-seo',
  ogImage: '/images/team-hero.png',
  ogType: 'website',
};

const hero = {
  title: 'Podiatry SEO That Fills Your Appointment Book',
  subtitle: 'Attract more patients searching for foot care, orthotics, sports podiatry, and specialized treatments. Dominate local search and build a thriving practice.',
  primaryCTA: {
    text: 'Free Strategy Session',
    href: '/contact',
