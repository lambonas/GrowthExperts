      </div>
    </section>

    <!-- Final CTA Section -->
    {sections.showFinalCTA && (
      <CTASection
        title={finalCTA.title}
        subtitle={finalCTA.subtitle}
        primaryCTA={finalCTA.primaryCTA}
        secondaryCTA={finalCTA.secondaryCTA}
      />
    )}
  </main>

  <Footer />
</BaseLayout>

<style>
  .section-padding {
    padding: 5rem 0;
  }

  @media (min-width: 768px) {
    .section-padding {
      padding: 7rem 0;
    }
  }

  @media (min-width: 1024px) {
