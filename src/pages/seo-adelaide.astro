---
import BaseLayout from '../layouts/BaseLayout.astro';
import Header from '../components/layout/Header.astro';
import Footer from '../components/layout/Footer.astro';
import HeroHome from '../components/sections/HeroHome.astro';
import ClientLogoCarousel from '../components/sections/ClientLogoCarousel.astro';
import CaseStudiesSection from '../components/sections/CaseStudiesSection.astro';
import CTASection from '../components/sections/CTASection.astro';
import SchemaOrg from '../components/seo/SchemaOrg.astro';

// ===================================
// PAGE FRONTMATTER CONFIGURATION
// Edit these values to customize the page content
// ===================================

// SEO Meta Data
const seo = {
  title: 'SEO Adelaide | Leading SEO Agency & Services in Adelaide',
  description: 'Expert SEO Adelaide services that drive real results. Our Adelaide SEO specialists deliver customised search engine optimisation strategies to boost rankings and grow your business.',
  keywords: [
    'seo adelaide',
    'SEO Agency Adelaide',
    'SEO company Adelaide',
    'seo services adelaide',
    'local seo adelaide',
    'local seo services adelaide',
    'adelaide seo',
    'search engine optimisation adelaide',
    'seo specialist adelaide',
    'seo expert adelaide'
  ],
  ogImage: '/images/team-hero.png',
  ogType: 'website',
};

// Hero Section
const hero = {
  title: 'SEO Adelaide',
  subtitle: 'Partner with Adelaide\'s leading SEO agency to dominate search rankings and drive qualified traffic to your business. Our proven search engine optimisation strategies deliver measurable results for Adelaide businesses.',
  primaryCTA: {
    text: 'Free Strategy Session',
    href: '/contact/'
  },
};

// Breadcrumbs
const breadcrumbs = [
  { label: 'Home', href: '/' },
  { label: 'Services', href: '/services/' },
  { label: 'SEO Adelaide', href: '/seo-adelaide/' },
];

// Section Visibility (show/hide sections)
const sections = {
  showClientLogos: true,
  showCaseStudies: true,
  showWhatsIncluded: true,
  showProcess: true,
  showFinalCTA: true,
};

// Final CTA Section
const finalCTA = {
  title: 'Ready to Dominate Search Rankings in Adelaide?',
  subtitle: 'Get your free SEO audit and discover the exact strategies we\'ll use to boost your rankings, increase traffic, and grow your Adelaide business. No obligations, just actionable insights from our expert SEO team.',
  primaryCTA: {
    text: 'Free Strategy Session',
    href: '/contact/'
  },
};
---

<BaseLayout
  title={seo.title}
  description={seo.description}
  keywords={seo.keywords}
  ogImage={seo.ogImage}
  ogType={seo.ogType}
  schema={{
    type: 'Service',
    data: {
      '@type': 'Service',
      name: 'SEO Adelaide Services',
      description: seo.description,
      provider: {
        '@type': 'Organization',
        name: 'Digital Growth Engine',
      },
      areaServed: {
        '@type': 'City',
        name: 'Adelaide',
        containedIn: {
          '@type': 'State',
          name: 'South Australia',
        },
      },
      hasOfferCatalog: {
        '@type': 'OfferCatalog',
        name: 'SEO Services Adelaide',
        itemListElement: [
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Local SEO Adelaide',
            },
          },
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'Technical SEO Services',
            },
          },
          {
            '@type': 'Offer',
            itemOffered: {
              '@type': 'Service',
              name: 'On-Page SEO Optimisation',
            },
          },
        ],
      },
    },
  }}
>
  <!-- Add LocalBusiness schema for better local SEO -->
  <SchemaOrg
    slot="head"
    type="LocalBusiness"
    data={{
      name: 'Digital Growth Engine',
      description: 'Expert SEO and digital marketing services in Adelaide, helping businesses dominate search rankings and grow online',
      image: `${Astro.site}images/logo.png`,
    }}
  />

  <Header />

  <main id="main-content">
    <!-- Hero Section - Conversion-focused with clear value proposition -->
    <HeroHome
      title={hero.title}
      subtitle={hero.subtitle}
      primaryCTA={hero.primaryCTA}
      secondaryCTA={hero.secondaryCTA}
    />

    <!-- Social Proof - Client Logos -->
    {sections.showClientLogos && <ClientLogoCarousel />}

    <!-- Main Content Section - Introduction to SEO Adelaide -->
    <section class="section-padding bg-white">
      <div class="container-custom">
        <div class="max-w-4xl mx-auto">
          <!-- Section Header -->
          <div class="text-center mb-12">
            <h2 class="text-3xl md:text-4xl lg:text-5xl font-bold font-display text-gray-900 mb-4">
              Why Choose Our SEO Agency Adelaide for Your Business Growth
            </h2>
            <p class="text-lg md:text-xl text-gray-600 leading-relaxed">
              Adelaide businesses need more than basic SEO. You need strategic search engine optimisation that delivers real results.
            </p>
          </div>

          <!-- Introductory Content -->
          <div class="prose prose-lg max-w-none mb-12">
            <p class="text-gray-700 leading-relaxed mb-4">
